package selfprogramming;

import java.util.Scanner;

public class SP6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner input = new Scanner(System.in);
		
		final int SIZE = 6;

	      for (int i = 1; i <= SIZE; i++)
	      {
	         for (int j = 1; j <= i; j++)
	         {
	            System.out.print("AISYAH");
	         }

	         System.out.println();
	      }

	}

}
